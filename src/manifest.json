{
	"manifest_version": 2,
	"name": "Redirect",
	"version": "0.1",
	"description": "Helps you focus on homework by redirecting you from distracting websites",
	"content_scripts": [{
		"matches": ["*://*/*"],
		"css": ["css/style.css"],
		"js": ["lib/jquery.min.js", "js/content_script.js"],
		"run_at": "document_end"
	}],
	"version_name": "v0.1 Tiny Bat",
	"browser_action": {
		"default_icon": "asset/icon.png",
		"default_popup": "view/popup.html"
	},
	"permissions": [
    "storage"
  ],
	"storage": {
	  "name": "Redirect",
	  "properties": {
	    "distractions": {
	      "type": "array",
	      "required": true,
	      "items": {
	        "type": "string"
	      }
	    },
			"focus": {
				"type": "string",
				"required": true
			},
			"prevWebsite": {
				"type": "string"
			},
			"breakInfo": {
				"type": "object",
				"properties": {
					"breakStart": {
						"type": "number"
					},
					"breakLength": {
						"type": "number"
					},
					"isOnBreak": {
						"type": "boolean"
					}
				}
			}
	  }
	}
}
